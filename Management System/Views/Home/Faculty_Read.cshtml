@model IEnumerable<Management_System.Models.Faculty>

@{
    ViewBag.Title = "Faculty_Read";
}

<h2>Faculty_Read</h2>

<div>
    <h4>Faculty</h4>
    <hr />
    <h4>@ViewBag.Message</h4>
    <table border="1">
        <tr>
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.FacultyName)
            </th>
            &nbsp;
            <th style="text-align: center">
                @Html.DisplayNameFor(model => model.Email)
            </th>
            &nbsp;
            <th style="text-align: center" width="170">
                @Html.DisplayNameFor(model => model.Password)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.Mobile)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.Active)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.createdOn)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.modifiedOn)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.deletedOn)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.isDeleted)
            </th>
            @*&nbsp;
                <th style="text-align: center" width="150">
                    @Html.DisplayNameFor(model => model.FacultyGuid)
                </th>*@
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.SubjectId)
            </th>
            @*&nbsp;
            <th style="text-align: center" width="150">
                <h5><b>Quick Actions</b></h5>
            </th>*@
        </tr>
        @foreach (Management_System.Models.Faculty faculty in Model)
        {
            <tr>
                <td style="text-align: center">
                    @faculty.FacultyName
                </td>
                <td style="text-align: center">
                    @faculty.Email
                </td>
                <td style="text-align: center">
                    @faculty.Password
                </td>
                <td style="text-align: center">
                    @faculty.Mobile
                </td>
                <td style="text-align: center">
                    @faculty.Active
                </td>
                <td style="text-align: center">
                    @faculty.createdOn
                </td>
                <td style="text-align: center">
                    @faculty.modifiedOn
                </td>
                <td style="text-align: center">
                    @faculty.deletedOn
                </td>
                <td style="text-align: center">
                    @faculty.isDeleted
                </td>
                @*<td style="text-align: center">
                        @faculty.FacultyGuid
                    </td>*@
                <td style="text-align: center">
                    @faculty.SubjectId
                </td>
                @*<td style="text-align: center">
                    @Html.ActionLink("Edit", "Faculty_Edit", new { id = faculty.FacultyId }) |
                    @Html.ActionLink("Delete", "Faculty_Delete", new { id = faculty.FacultyId }) |
                    @Html.ActionLink("Details", "Faculty_ReadById", new { id = faculty.FacultyId })
                </td>*@
            </tr>
        }
    </table>
</div>
