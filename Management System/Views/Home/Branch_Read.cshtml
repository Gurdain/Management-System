@model IEnumerable<Management_System.Models.Branch>

@{
    ViewBag.Title = "Branch_Read";
}

<h2>Branch_Read</h2>

<div>
    <h4>Branch</h4>
    <hr />
    <table border="1">
        <tr>
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.BranchName)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.createdOn)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.modifiedOn)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.deletedOn)
            </th>
            &nbsp;
            <th style="text-align: center" width="150">
                @Html.DisplayNameFor(model => model.isDeleted)
            </th>
            @*&nbsp;
            <th style="text-align: center" width="150">
                <h5><b>Quick Actions</b></h5>
            </th>*@
        </tr>
        @foreach (Management_System.Models.Branch branch in Model)
        {
            <tr>
                <td style="text-align: center">
                    @branch.BranchName
                </td>
                <td style="text-align: center">
                    @branch.createdOn
                </td>
                <td style="text-align: center">
                    @branch.modifiedOn
                </td>
                <td style="text-align: center">
                    @branch.deletedOn
                </td>
                <td style="text-align: center">
                    @branch.isDeleted
                </td>
                @*<td style="text-align: center">
                    @Html.ActionLink("Edit", "Branch_Edit", new { id = branch.BranchId }) |
                    @Html.ActionLink("Delete", "Branch_Delete", new { id = branch.BranchId }) |
                    @Html.ActionLink("Details", "Branch_ReadById", new { id = branch.BranchId })
                </td>*@
            </tr>
        }
    </table>
</div>
